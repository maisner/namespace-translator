{
  "name": "maisner/namespace-translator",
  "description": "Manage Nette app translations on per namespace basis.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wavevision s.r.o",
      "email": "info@wavevision.com"
    },
    {
      "name": "Jakub Filla",
      "email": "jakub.filla@wavevision.com"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "contributte/console": "^0.8.0",
    "contributte/translation": "^0.7.1",
    "google/apiclient": "^2.7",
    "nette/application": "^3.0",
    "nette/di": "^3.0",
    "nette/neon": "^3.0",
    "nikic/php-parser": "^4.9",
    "wavevision/di-service-annotation": "^4.0",
    "wavevision/utils": "^2.1"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "php-coveralls/php-coveralls": "^2.2",
    "wavevision/coding-standard": "^5.2",
    "wavevision/nette-tests": "^3.1"
  },
  "extra": {
    "phpstan": {}
  }
}
